  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/react-portal": "4.0.4",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "jest": "^29.6.2",
    "prettier": "^2.3.2",
